

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh "sudo python3 python1.py"
            }
        }
        stage('build') {
            steps {
			sh 'sudo docker build -t  myimg:v1 .'
                
            }
        }
        stage('run') {
            steps {
                sh 'sudo docker run --name you -p 8101:80 -d myimg:v1'
            }
        }
   }
}




